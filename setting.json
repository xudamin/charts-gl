{
  "git.enableSmartCommit": true,
  "files.associations": {
    ".eslintrc": "json",
    "*.vue": "vue",
    "*.postcss": "css",
    "*.css": "css",
    "*.html": "html"
  },
  "files.insertFinalNewline": true,
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "breadcrumbs.enabled": false,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html",
    {
      "language": "vue",
      "autoFix": true
    },
    "vue-html",
    {
      "language": "html",
      "autoFix": true
    }
  ],
  "eslint.options": {
    "configFile": ".eslintrc.js"
  },
  "eslint.autoFixOnSave": true,
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/NPv3demo": true,
    "**/dist": true
  },
  "cSpell.enabledLanguageIds": [
    "c",
    "cpp",
    "csharp",
    "go",
    "javascript",
    "javascriptreact",
    "json",
    "latex",
    "markdown",
    "php",
    "plaintext",
    "python",
    "text",
    "typescript",
    "typescriptreact",
    "yml"
  ],
  "cSpell.userWords": [
    "Cascader",
    "Vuex",
    "browserslist",
    "btns",
    "cn",
    "com",
    "commitlint",
    "dbappsecurity",
    "devtool",
    "he",
    "devin",
    "mixins",
    "nprogress",
    "portfinder",
    "stylelint",
    "vetur",
    "vsicons",
    "xnpm"
  ],
  "git.autofetch": true,
  "git.confirmSync": false,
  "editor.fontFamily": "Consolas, 'Courier New', monospace, '微软雅黑'"
}
